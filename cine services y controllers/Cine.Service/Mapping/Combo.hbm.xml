<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Cine.Service"
                   namespace="Cine.Service.Dominio"
                   default-lazy="false">
  <class name="Combo" table="tbCombo">
    <id name="idCombo" access="property" column="idCombo">
      <generator class="native" />
    </id>

    <property name="nombre" column="nombre" />
    <property name="descripcion" column="descripcion" />
    <property name="precioVenta" column="precioVenta" />
    <property name="imagen" column="imagen" />


  </class>
</hibernate-mapping>